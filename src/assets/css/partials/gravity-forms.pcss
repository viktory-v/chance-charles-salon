@import "../global-mixins.pcss";

.gform_wrapper {
  box-sizing: border-box;

  .gform_fields {
    margin: 0;
    padding: 0;
  }

  input {
    border: none;
    padding-left: 31px;
  }

  .validation_error {
    display: none;
  }

  .gfield_label {
    display: none;
  }

  .gfield_description {
    display: none;
  }
}
